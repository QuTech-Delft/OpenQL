{
    "eqasm_compiler" : "cc_light_compiler",

    "hardware_settings": {
        "qubit_number": 8,
        "cycle_time" : 1,
        "mw_mw_buffer": 0,
        "mw_flux_buffer": 0,
        "mw_readout_buffer": 0,
        "flux_mw_buffer": 0,
        "flux_flux_buffer": 0,
        "flux_readout_buffer": 0,
        "readout_mw_buffer": 0,
        "readout_flux_buffer": 0,
        "readout_readout_buffer": 0
