								[2, 8, 10, 14, 16, 22],											// [freq H]
								[1, 9, 13, 17], 													// [freq Mg]
								[7, 11, 15, 23]													// [freq My]
							]
						}
					},

					// flux
					{	"slot": 6,
						"io_module": "cc-conn-dio-diff",
						"instrument": {
							"name": "awg8-flux-0",
							"type": "zi-hdawg",
							"control_mode": "awg8-flux",
							"signal_type": "flux",
							"qubits": [[1], [2], [6], [7], [8], [9], [10], [11]]
						}
